CREATE TABLE IF NOT EXISTS user_tuners (
    user_id integer NOT NULL,
    tuner_id integer NOT NULL,
    CONSTRAINT user_tuners_pk PRIMARY KEY (tuner_id,user_id),
    CONSTRAINT user_tuners_users FOREIGN KEY (user_id)
    REFERENCES users (id),
    CONSTRAINT user_tuners_tuners FOREIGN KEY (tuner_id)
    REFERENCES tuners (id)
);